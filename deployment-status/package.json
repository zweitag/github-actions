{
  "name": "deployment-status",
  "version": "0.1.0",
  "description": "Creates a GitHub Deployment Status",
  "private": true,
  "main": "lib/initial-status.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build -o dist/initial-status src/initial-status.ts && ncc build -o dist/final-status src/final-status.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zweitag/github-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "deployment",
    "status"
  ],
  "author": "zweitag",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@octokit/types": "^6.40.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.15",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.20.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^24.7.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^3.14.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
