{
  "name": "deployment-status",
  "version": "0.1.0",
  "description": "Creates a GitHub Deployment Status",
  "private": true,
  "main": "lib/initial-status.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build -o dist/initial-status src/initial-status.ts && ncc build -o dist/final-status src/final-status.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zweitag/github-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "deployment",
    "status"
  ],
  "author": "zweitag",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0",
    "@octokit/types": "^5.5.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.11.1",
    "@vercel/ncc": "^0.24.0",
    "eslint": "^7.9.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.1",
    "jest": "^26.4.2",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3"
  }
}
