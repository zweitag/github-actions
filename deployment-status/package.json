{
  "name": "deployment-status",
  "version": "0.1.0",
  "description": "Creates a GitHub Deployment Status",
  "private": true,
  "main": "lib/initial-status.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build -o dist/initial-status src/initial-status.ts && ncc build -o dist/final-status src/final-status.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zweitag/github-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "deployment",
    "status"
  ],
  "author": "zweitag",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/github": "^6.0.1",
    "@octokit/types": "^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.12.0",
    "jest": "^30.2.0",
    "jest-circus": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
